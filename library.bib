pages = {e182--e189},
title = {{Genome-wide promoter DNA methylation dynamics of human hematopoietic progenitor cells during differentiation and aging}},
url = {http://www.bloodjournal.org/content/117/19/e182 http://www.bloodjournal.org/content/117/19/e182.short?sso-checked=true http://www.bloodjournal.org/content/bloodjournal/117/19/e182.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/21427290},
