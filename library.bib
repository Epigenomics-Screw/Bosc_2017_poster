title = {{A harmonized approach to intracellular cytokine staining gating: Results from an international multiconsortia proficiency panel conducted by the Cancer Immunotherapy Consortium (CIC/CRI)}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/cyto.a.22319/abstract http://onlinelibrary.wiley.com/doi/10.1002/cyto.a.22319/full http://onlinelibrary.wiley.com/store/10.1002/cyto.a.22319/asset/cytoa22319.pdf?v=1{\&}t=hvrbhjl4{\&}s=1fe6c894d7f856e70c22facf0f051e8c46ccacbe},
