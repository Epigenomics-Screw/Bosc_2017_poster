title = {{Sequence Features of Drosha and Dicer Cleavage Sites Affect the Complexity of IsomiRs}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4425070/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4425070/pdf/ijms-16-08110.pdf http://www.mdpi.com/1422-0067/16/4/8110/},
