issn = {0022-1759},
journal = {Journal of Immunological Methods},
keywords = {Automated analysis,ICS,Positivity,Rare events,Reproducibility,Standardization,standardization},
mendeley-tags = {Automated analysis,ICS,Positivity,Rare events,Reproducibility,standardization},
