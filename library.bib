issn = {1535-6108},
journal = {Cancer Cell},
language = {English},
month = {dec},
number = {6},
pages = {553--567},
title = {{Leukemic IDH1 and IDH2 Mutations Result in a Hypermethylation Phenotype, Disrupt TET2 Function, and Impair Hematopoietic Differentiation}},
url = {http://www.cell.com/article/S1535610810004836/abstract http://www.cell.com/cancer-cell/abstract/S1535-6108(10)00483-6},
