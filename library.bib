file = {:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Ehninger et al. - 2014 - Distribution and levels of cell surface expression of CD33 and CD123 in acute myeloid leukemia.pdf:pdf;:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Ehninger et al. - 2014 - Distribution and levels of cell surface expression of CD33 and CD123 in acute myeloid leukemia.html:html},
journal = {Blood Cancer Journal},
language = {en},
month = {jun},
number = {6},
pages = {e218},
title = {{Distribution and levels of cell surface expression of CD33 and CD123 in acute myeloid leukemia}},
url = {http://www.nature.com/bcj/journal/v4/n6/full/bcj201439a.html http://www.nature.com/bcj/journal/v4/n6/pdf/bcj201439a.pdf},
