issn = {1087-0156},
journal = {Nature Biotechnology},
language = {en},
month = {aug},
number = {8},
pages = {771--776},
title = {{Genome mapping on nanochannel arrays for structural variation analysis and sequence assembly}},
url = {http://www.nature.com/nbt/journal/v30/n8/full/nbt.2303.html},
volume = {30},
year = {2012}
}
