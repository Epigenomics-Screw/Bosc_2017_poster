journal = {FEBS Open Bio},
keywords = {C,HK1,Nasopharyngeal carcinoma,Nasopharyngeal cell lines/xenograft (NP460,Nasopharyngeal cell lines/xenograft (NP460- HK1- C,RNA sequencing,TP53,Transcriptome analysis},
