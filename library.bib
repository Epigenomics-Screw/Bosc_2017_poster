issn = {1559-2294},
journal = {Epigenetics},
keywords = {Allele-specific DNA methylation,Epigenetics,SNP,blood,brain,cerebellum,cortex,genomic imprinting},
mendeley-tags = {Allele-specific DNA methylation,Epigenetics,SNP,blood,brain,cerebellum,cortex,genomic imprinting},
month = {jan},
