@article{Zhou2015,
abstract = {Much-needed attention has been given of late to diseases specifically associated with an expanding elderly population. Myelodysplastic syndrome (MDS), a hematopoietic stem cell-based blood disease, is one of these. The lack of clear understanding of the molecular mechanisms underlying the pathogenesis of this disease has hampered the development of efficacious therapies, especially in the presence of co-morbidities. Mouse models could potentially provide new insights into this disease, although primary human MDS cells grow poorly in xenografted mice. This makes genetically-engineered murine models a more attractive proposition, although this approach is not without complications. In particular, it is unclear if or how myelodysplasia (abnormal blood cell morphology), a key MDS feature in humans, presents in murine cells. Here, we evaluate the histopathological features of wild-type mice and 23 mouse models with verified myelodysplasia. We find that certain features indicative of myelodysplasia in humans, such as Howell-Jolly bodies and low neutrophilic granularity, are commonplace in healthy mice, while other features are similarly abnormal in humans and mice. Quantitative hematopoietic parameters, such as blood cell counts, are required to distinguish between MDS and related diseases. We provide data that mouse models of MDS can be genetically engineered and faithfully recapitulate human disease.},
author = {Zhou, Ting and Kinney, Marsha C and Scott, Linda M and Zinkel, Sandra S and Rebel, Vivienne I},
doi = {10.1182/blood-2015-01-624239},
file = {:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Zhou et al. - 2015 - Revisiting the case for genetically engineered mouse models in human myelodysplastic syndrome research.pdf:pdf},
issn = {1528-0020},
journal = {Blood},
month = {jun},
pmid = {26077396},
title = {{Revisiting the case for genetically engineered mouse models in human myelodysplastic syndrome research.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26077396},
volume = {Digital pr},
year = {2015}
}
