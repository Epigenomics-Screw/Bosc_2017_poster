month = {jun},
number = {6},
pages = {1694--1703},
title = {{A role for $\gamma$/$\delta$ T cells in a mouse model of fracture healing}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/art.24520/abstract http://onlinelibrary.wiley.com/store/10.1002/art.24520/asset/24520{\_}ftp.pdf?v=1{\&}t=hw1ca8yk{\&}s=c15c5af4f50ab87459aeb22203cec17357d05f57},
