pages = {e55534},
title = {{Contribution of Multiparameter Flow Cytometry Immunophenotyping to the Diagnostic Screening and Classification of Pediatric Cancer}},
url = {http://dx.doi.org/10.1371/journal.pone.0055534 http://www.plosone.org/article/fetchObject.action?uri=info:doi/10.1371/journal.pone.0055534{\&}representation=PDF http://www.plosone.org/article/info:doi/10.1371/journal.pone.0055534},
volume = {8},
