issn = {0732-183X, 1527-7755},
journal = {Journal of Clinical Oncology},
language = {en},
month = {may},
number = {10},
pages = {2480--2485},
title = {{Comparison of Cytogenetic and Molecular Cytogenetic Detection of Chromosome Abnormalities in 240 Consecutive Adult Patients With Acute Myeloid Leukemia}},
url = {http://jco.ascopubs.org/content/20/10/2480 http://jco.ascopubs.org/content/20/10/2480.full http://jco.ascopubs.org/content/20/10/2480.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/12011125},
