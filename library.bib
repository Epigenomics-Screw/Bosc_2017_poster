pages = {610--621},
pmid = {18285502},
title = {{Application of massively parallel sequencing to microRNA profiling and discovery in human embryonic stem cells.}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2279248/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2279248/pdf/610.pdf},
volume = {18},
