issn = {1367-4803, 1460-2059},
journal = {Bioinformatics},
language = {en},
month = {aug},
number = {16},
pages = {1988--1997},
title = {{Clustering microarray gene expression data using weighted Chinese restaurant process}},
url = {http://bioinformatics.oxfordjournals.org/content/22/16/1988 http://bioinformatics.oxfordjournals.org/content/22/16/1988.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/16766561},
