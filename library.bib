series = {(Epi)Genomics approaches and their applications},
title = {{Single molecule and single cell epigenomics}},
url = {http://www.sciencedirect.com/science/article/pii/S1046202314002825 http://www.ncbi.nlm.nih.gov/pubmed/25204781},
volume = {72},
year = {2014}
