issn = {1552-4930},
journal = {Cytometry Part A},
keywords = {Folder - VanBUG talk,bioinformatics,cellular hierarchy,exploratory data analysis,gating,graph theory,marker panel,mass cytometry,polychromatic flow cytometry,statistics},
language = {en},
