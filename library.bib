title = {{MicroRNA heterogeneity in endometrial cancer cell lines revealed by deep sequencing}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4665306/ http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4665306/pdf/ol-10-06-3457.pdf},
volume = {10},
