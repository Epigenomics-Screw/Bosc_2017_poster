file = {:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Hensor et al. - 2014 - A1.33 Predicting the evolution of inflammatory arthritis in ACPA-positive individuals can T-cell subsets help.html:html},
issn = {, 1468-2060},
