pages = {128--135},
title = {{Single-cell technologies for monitoring immune systems}},
url = {http://www.nature.com/ni/journal/v15/n2/full/ni.2796.html http://www.nature.com/ni/journal/v15/n2/pdf/ni.2796.pdf},
volume = {15},
year = {2014}
}
@article{Wu2015,
abstract = {DNA methylation is an important epigenetic modification involved in many biological processes and diseases. Recent developments in whole genome bisulfite sequencing (WGBS) technology have enabled genome-wide measurements of DNA methylation at single base pair resolution. Many experiments have been conducted to compare DNA methylation profiles under different biological contexts, with the goal of identifying differentially methylated regions (DMRs). Due to the high cost of WGBS experiments, many studies are still conducted without biological replicates. Methods and tools available for analyzing such data are very limited.
We develop a statistical method, DSS-single, for detecting DMRs from WGBS data without replicates. We characterize the count data using a rigorous model that accounts for the spatial correlation of methylation levels, sequence depth and biological variation. We demonstrate that using information from neighboring CG sites, biological variation can be estimated accurately even without replicates. DMR detection is then carried out via a Wald test procedure. Simulations demonstrate that DSS-single has greater sensitivity and accuracy than existing methods, and an analysis of H1 versus IMR90 cell lines suggests that it also yields the most biologically meaningful results. DSS-single is implemented in the Bioconductor package DSS.},
author = {Wu, Hao and Xu, Tianlei and Feng, Hao and Chen, Li and Li, Ben and Yao, Bing and Qin, Zhaohui and Jin, Peng and Conneely, Karen N.},
doi = {10.1093/nar/gkv715},
file = {:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Wu et al. - 2015 - Detection of differentially methylated regions from whole-genome bisulfite sequencing data without replicates.pdf:pdf;:home/koneill/Library/Application Support/Mendeley Desktop/Downloaded/Wu et al. - 2015 - Detection of differentially methylated regions from whole-genome bisulfite sequencing data without replicates.html:html},
issn = {0305-1048, 1362-4962},
journal = {Nucleic Acids Research},
language = {en},
month = {jul},
pages = {gkv715},
title = {{Detection of differentially methylated regions from whole-genome bisulfite sequencing data without replicates}},
url = {http://nar.oxfordjournals.org/content/early/2015/07/15/nar.gkv715 http://nar.oxfordjournals.org/content/early/2015/07/15/nar.gkv715.full.pdf http://nar.oxfordjournals.org/content/early/2015/07/15/nar.gkv715.long http://www.ncbi.nlm.nih.gov/pubmed/26184873},
