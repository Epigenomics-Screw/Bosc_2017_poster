title = {{Tissue-specific patterns of allelically-skewed DNA methylation}},
url = {http://dx.doi.org/10.1080/15592294.2015.1127479 http://www.tandfonline.com/doi/full/10.1080/15592294.2015.1127479?scroll=top{\&}needAccess=true http://www.tandfonline.com/doi/pdf/10.1080/15592294.2015.1127479},
volume = {11},
