issn = {1538-4101},
journal = {Cell Cycle},
month = {jul},
number = {14},
pages = {2339--2343},
title = {{Azacitidine-resistant SKM1 myeloid cells are defective for AZA-induced mitochondrial apoptosis and autophagy}},
url = {http://dx.doi.org/10.4161/cc.10.14.16308 http://www.tandfonline.com/doi/abs/10.4161/cc.10.14.16308 http://www.tandfonline.com/doi/pdf/10.4161/cc.10.14.16308},
volume = {10},
