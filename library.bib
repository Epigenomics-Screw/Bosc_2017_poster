issn = {0022-1899, 1537-6613},
journal = {Journal of Infectious Diseases},
language = {en},
month = {jan},
number = {2},
pages = {272--284},
title = {{Immunologic and Virologic Events in Early HIV Infection Predict Subsequent Rate of Progression}},
