journal = {Nature reviews. Genetics},
keywords = {Animals,Cell Separation,Cell Separation: methods,Epigenomics,Gene Expression Profiling,Genomics,High-Throughput Nucleotide Sequencing,High-Throughput Nucleotide Sequencing: methods,Humans,Proteomics,Single-Cell Analysis,Single-Cell Analysis: methods},
