issn = {1097-0320},
journal = {Cytometry},
keywords = {CD34,CD4,Folder - flow background,absolute cell count,counting beads,flow cytometry (FCM),immunofluorescence (IF),leukoreduced blood products,monoclonal antibodies,single platform},
language = {en},
mendeley-tags = {CD34,CD4,Folder - flow background,absolute cell count,counting beads,flow cytometry (FCM),immunofluorescence (IF),leukoreduced blood products,monoclonal antibodies,single platform},
